package com.sist.dao;

import com.mongodb.*;

//★★★ [MongoDB] ★★★
public class MovieDAO {
	private MongoClient mc; // Connection
	private DB db; // DataBase(XE)
	private DBCollection dbc; // Table
	public MovieDAO()
	{
		try {
			mc=new MongoClient("localhost",27017); // Oracle의 1521처럼 MongoDB는 27017
			
		} catch (Exception ex) {}
	}
}
